{
  "name": "sub-bridge",
  "version": "1.1.1",
  "description": "MCP bridge to use ChatGPT Pro, Claude Max, etc. in Cursor via an OpenAI-compatible proxy",
  "main": "dist/cli.js",
  "bin": {
    "sub-bridge": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts --verbose",
    "dev:server": "tsx watch src/server.ts",
    "dev:mcp": "tsx src/mcp.ts",
    "dev:tmux": "tmux attach -t sub-bridge 2>/dev/null || tmux new -s sub-bridge \"set -a; source ./.env; set +a; npm run dev:server || sleep 10\"",
    "start": "node dist/cli.js",
    "start:server": "tsx src/server.ts",
    "start:mcp": "tsx src/mcp.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cursor",
    "claude",
    "anthropic",
    "openai",
    "proxy",
    "mcp",
    "api",
    "connector",
    "code-editor",
    "claude-pro",
    "claude-max",
    "oauth"
  ],
  "author": "Maol-1997",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/buremba/sub-bridge"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.7",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@sentry/node": "^10.32.1",
    "chalk": "^5.6.2",
    "cloudflared": "^0.7.1",
    "commander": "^14.0.2",
    "hono": "^4.8.3",
    "openai": "^6.15.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.18",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
